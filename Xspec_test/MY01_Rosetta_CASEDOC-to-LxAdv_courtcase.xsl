        &lt;entry align="left"&gt;page 727
          &lt;page reporter="nzlr" count="498" text="[1998] 2 NZLR 481 page "/&gt;
        &lt;/entry&gt;
      &lt;/row&gt;
      &lt;!-- .... --&gt;
