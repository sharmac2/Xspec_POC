    <xsl:key name="idTypeValue" match="*[@ID]" use="@ID"/>-->
   <xsl:template match="remotelink[key('idTypeValue', @refpt)[self::refpt]][not(ancestor::ci:content)][not(parent::source_cttr:annot and $streamID=('HK08' , 'AU02', 'AU04','UK05','CA03'))]"
                 priority="25">
      <ref:lnlink>
