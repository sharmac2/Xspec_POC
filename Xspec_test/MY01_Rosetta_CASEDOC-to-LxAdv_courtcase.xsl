                                 </ref:anchor>
                                 <bodytext>
                                    <xsl:for-each select="current-group()">
                                       <xsl:apply-templates select="@* | node()"/>
                                    </xsl:for-each>
                                 </bodytext>
                              </marginnote>
                           </text>
                        </xsl:when>
                     </xsl:choose>
                  </xsl:for-each-group>
               </xsl:if>
            </p>
            <xsl:for-each select="following-sibling::level">
               <xsl:call-template name="following_p_level"/>
            </xsl:for-each>
         </xsl:otherwise>
      </xsl:choose>
   </xsl:template>

