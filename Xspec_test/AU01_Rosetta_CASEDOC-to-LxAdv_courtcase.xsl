   <xsl:variable name="dateTime"
                 as="xs:string"
                 select=" translate( substring( string(current-dateTime()) , 1 , 19 ) , 'T' , ' ' ) "/>
   <!--  Helpfule for Fab pipeline processing to indicate bundle ID   -->
