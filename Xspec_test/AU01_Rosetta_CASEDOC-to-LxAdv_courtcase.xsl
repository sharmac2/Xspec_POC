   <!-- JL: keys for testing 

    <xsl:key name="idTypeValue" match="*[@id]" use="@id"/>
    <xsl:key name="idTypeValue" match="*[@ID]" use="@ID"/>
    <xsl:key name="idTypeValue" match="footnote" use="@fntoken"/>
