   <!-- Added template for UK15 by Priya kaushal -->
   <xsl:template name="bodytext-following-level">
      <xsl:choose>
         <xsl:when test="child::heading/title/refpt">
            <seclaw:level>
               <xsl:attribute name="leveltype">
                  <xsl:value-of select="@leveltype"/>
