      <!--  prepend underscore with leading values of period, hyphen, or numeral -->
      <xsl:choose>
         <xsl:when test="matches($idWoInternalSpaces , '^[0-9]|^\.|^\-')">
            <xsl:value-of select="concat('_', $idWoInternalSpaces)"/>
         </xsl:when>
