	</xsl:template-->   <!-- rosetta element: docinfo:doc-heading -->   <!-- <topicref href="../../common_newest/Rosetta_docinfo.doc-heading-LxAdv-dc.title.dita"/> -->   <xd:doc>
      <xd:desc>
         <xd:p>
            <xd:b>Instructions <xd:i>[common element]</xd:i>
            </xd:b>
         </xd:p>
         <xd:p>
