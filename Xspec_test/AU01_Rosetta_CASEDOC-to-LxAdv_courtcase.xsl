               <ref:locator>
                  <ref:locator-key>
                     <ref:key-name>
                        <xsl:attribute name="name">
                           <xsl:value-of select="key-name[@name='queryTemplatePGUID']/@name"/>
                        </xsl:attribute>
                     </ref:key-name>
                     <ref:key-value>
                        <xsl:attribute name="value">
                           <xsl:value-of select="key-value/@value"/>
                        </xsl:attribute>
                     </ref:key-value>
                  </ref:locator-key>
                  <xsl:apply-templates select="api-params"/>
