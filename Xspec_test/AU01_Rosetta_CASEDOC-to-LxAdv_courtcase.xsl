								<xd:b>doc:legacy-metadata[@metadatasource="lbu-meta"]</xd:b> with child elements: <xd:ul>
                     <xd:li>Create <xd:b>meta</xd:b> and <xd:b>metaitem</xd:b> with attributes
										<xd:b>@name</xd:b> and <xd:b>@value</xd:b>. </xd:li>
