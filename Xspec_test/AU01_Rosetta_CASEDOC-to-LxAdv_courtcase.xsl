         <xd:pre>

&lt;docinfo:versioninfo hasotherversions="true"&gt;
    &lt;docinfo:startdate day="01" month="01" year="2003"/&gt;
    &lt;docinfo:enddate day="30" month="06" year="2003"/&gt;
&lt;/docinfo:versioninfo&gt;
