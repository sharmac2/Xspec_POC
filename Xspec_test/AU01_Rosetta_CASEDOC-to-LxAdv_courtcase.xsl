               <xd:li>
                  <xd:i>inlineobject</xd:i> to
                        <xd:b>ref:inlineobject</xd:b> for BLOBSTORE images
                        (id-CCCC-10540)</xd:li>
               <xd:li>
                  <xd:i>link[@filename]</xd:i> to
                        <xd:b>ref:lnlink[@service="ATTACHMENT"]</xd:b> for BLOBSTORE
                        images (id-CCCC-10541)</xd:li>
            </xd:ul>
         </xd:p>
         <xd:p>
