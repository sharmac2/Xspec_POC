         <xd:p>
            <xd:ul>
               <xd:li>
                  <xd:i>docinfo</xd:i> becomes <xd:b>doc:metadata/doc:docinfo</xd:b>.
								<xd:i>docinfo:dpsi[@id-string]</xd:i>, <xd:i>docinfo[@browseprev]</xd:i>,
								<xd:i>docinfo[@browsenext]</xd:i>, <xd:i>docinfo[@partitionnum]</xd:i>,
