					&lt;/ci:content&gt; &lt;/ci:cite&gt; </xd:pre>
         <xd:p>
            <xd:b>Target XML</xd:b>
         </xd:p>
         <xd:pre> &lt;lnci:cite&gt; &lt;lnci:case&gt; &lt;lnci:caseref
					ID="cref00000047"&gt; &lt;lnci:reporter value="OEAB"/&gt;
