               <xsl:apply-templates select="heading"/>
               <bodytext>
                  <xsl:apply-templates select="node() except heading"/>
               </bodytext>
            </ref:relatedrefs>
         </xsl:otherwise>
      </xsl:choose>
   </xsl:template>

