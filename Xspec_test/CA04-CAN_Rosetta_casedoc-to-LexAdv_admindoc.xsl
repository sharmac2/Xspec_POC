                                <xsl:value-of select="number(regex-group(1)) * 1"/>
                            </xsl:variable>
                            <xsl:value-of select="concat($var1, '*')"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:value-of select="concat(regex-group(1), '*')"/>
