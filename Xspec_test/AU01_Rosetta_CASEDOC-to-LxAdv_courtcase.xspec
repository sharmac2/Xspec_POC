            " test="/courtcase:courtcase/courtcase:body[1]/courtcase:opinions[1]/courtcase:prelim[1]/base:bodytext[1]/base:toc[1]/base:toc-entry[38]/ref:page[1]">
            <ref:page num="577" page-scheme="ald" includeintoc="true" page-scheme-type="reporter-abbreviation"/>
        </x:expect>
        <x:expect label="//ref:page/@num='577' should be created only once" test="count(//ref:page/@num='577') = 1"/>
        </x:scenario>
    <x:scenario label="Scenario for testing W*7137100 | Reporter line suppressed in LA platform">
        <x:context>
            <case:author>
                <person>
                    <name.text>JUSTIN CARTER</name.text>
                    <role>BARRISTER</role>
                </person>
            </case:author>
        </x:context>
        <x:expect label="for the working scenario , we should have the person:contributor element with contributor-type as author">
            <note xmlns="http://www.lexisnexis.com/xmlschemas/content/shared/base/1/">
                <byline>
                    <person:contributor contributor-type="author">
                        <person:person>
                            <person:name.text>JUSTIN CARTER</person:name.text>
                            <role>BARRISTER</role>
                        </person:person>
                    </person:contributor>
                </byline>
            </note>
        </x:expect>
    </x:scenario>
